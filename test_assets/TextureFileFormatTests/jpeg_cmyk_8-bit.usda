#usda 1.0
(
    defaultPrim = "Scene"
    upAxis = "Y"
    metersPerUnit = 1
    customLayerData = {
        string copyright = "Copyright 2024 Apple Inc. All rights reserved."
    }
    subLayers = [
        @_scene.usda@
    ]
)

over "Scene"
{
    over "Looks"
    {
        over "MainMaterial"
        {
            over "Diffuse"
            {
                asset inputs:file = @jpeg_cmyk_8-bit.jpg@
            }
        }

        over "MainMaterialWithOpacity"
        {
            over "Diffuse"
            {
                asset inputs:file = @jpeg_cmyk_8-bit.jpg@
            }
        }
    }
}
